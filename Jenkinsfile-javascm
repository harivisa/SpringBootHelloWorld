pipeline {
    agent any 
    stages {
        stage('Clone repo and clean') { 
            steps {
                sh "mvn clean"
            }
        }
        stage('Test') { 
            steps {
                sh "mvn test"
            }
        }
        stage('Deploy') { 
            steps {
                 sh "mvn install"
            }
        }
    }
}
