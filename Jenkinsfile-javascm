pipeline {
    agent any 
    stages {
        stage('Clone git and clean') { 
            steps {
                sh "mvn clean"
            }
        }
        stage('Build and Test') { 
            steps {
                sh "mvn test"
            }
        }
        stage('Ready to deploy') { 
            steps {
                 sh "mvn install"
            }
        }
    }
}
